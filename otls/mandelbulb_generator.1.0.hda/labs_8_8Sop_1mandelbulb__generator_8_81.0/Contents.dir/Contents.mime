#bind parm nvalue float 
#bind parm iterations int
#bind parm maxiterations int
#bind parm phase float 
#bind parm power float 

int mandelbrot(float3 orig, int imax, float n, float phase, float power, int iterations) 
{

    float x, y, z;
    float xnew, ynew, znew;
    float3 pos = orig;
    float r, theta, phi;
    
    x = pos.x;
    y = pos.y;
    z = pos.z;
    
    for(int i = 0 ; i < imax; ++i) 
    {
    
        r = length((float3)(x, y, z));
        theta = power * acos(z / r) + phase;
        phi = atan(y / x);
         
        xnew = pow(r, n) * sin(theta * n) * cos(phi * n) + pos.x ;
        ynew = pow(r, n) * sin(theta * n) * sin(phi * n) + pos.y ;
        znew = pow(r, n) * cos(theta * n) + pos.z ;        
        
        if((pow(xnew, 2) + pow(ynew, 2) + pow(znew, 2)) > iterations)
            
            return i;

        
        x = xnew;
        y = ynew;
        z = znew;
            
    }    
                
       return imax;

}


@KERNEL
{
      
    float3 orig = @density.pos;
        
    int val = (mandelbrot(orig, @maxiterations, @nvalue, @phase, @power, @iterations)) < @maxiterations;
    
    float newdensity = 1.0 - (float)val;
    
    float xcheck = fabs(orig.x);
    float ycheck = fabs(orig.y);
    float tol = 1e-6;        
    if((xcheck < tol) && (ycheck < tol))
        newdensity = 0.0f;
    
    @density.set(newdensity);

}
