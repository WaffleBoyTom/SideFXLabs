--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/mandelbulb.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	""	)
bindeach	[ 0	locks=0 ]	(	"off"	)
snippet	[ 0	locks=0 ]	(	"function int mandelbrot(const vector orig; const int imax) 
{

    float x, y, z; 
    float xnew, ynew, znew;
    float r, theta, phi;
    float n = chf('nvalue');    
    float phase = chf('phase');
    float power = chf('power');
        
    x = orig.x;
    
    y = orig.y;
    
    z = orig.z;
    
    for(int i = 0 ; i < imax ; i++) 
    {
    
       
        r = length(set(x, y, z));
        theta = power * acos(z / r) + phase;
        phi = atan2(y, x);
    
        xnew = pow(r, n) * sin(theta * n) * cos(phi * n) + orig.x ;
        ynew = pow(r, n) * sin(theta * n) * sin(phi * n) + orig.y ;
        znew = pow(r, n) * cos(theta * n) + orig.z ;
        
        if(pow(xnew,2) + pow(ynew,2) + pow(znew,2) > chf('iteration'))
            
            return i;

        
        x = xnew;
        y = ynew;
        z = znew;
    
    }
    
    return imax;

}

int maxiter = chi('maxiter');

f@density = 1.0 - (mandelbrot(v@P, maxiter) < maxiter);
"	)
exportlist	[ 0	locks=0 ]	(	*	)
vex_strict	[ 0	locks=0 ]	(	"off"	)
autobind	[ 0	locks=0 ]	(	"on"	)
bindings	[ 0	locks=0 ]	(	0	)
vex_geometrygenerator	[ 0	locks=0 ]	(	"off"	)
vdb_signedflood	[ 0	locks=0 ]	(	"on"	)
vex_cwdpath	[ 0	locks=0 ]	(	.	)
vex_outputmask	[ 0	locks=0 ]	(	*	)
vex_precision	[ 0	locks=0 ]	(	auto	)
folder1	[ 0	locks=0 ]	(	0	0	)
folder_generatedparms_snippet	[ 0	locks=0 ]	(	0	)
nvalue	[ 0	locks=0 ]	(	[ nvalue	0 ] 	)
iteration	[ 0	locks=0 ]	(	[ iteration	0 ] 	)
maxiter	[ 0	locks=0 ]	(	[ maxiter	0 ] 	)
phase	[ 0	locks=0 ]	(	[ phase	0 ] 	)
power	[ 0	locks=0 ]	(	[ power	0 ] 	)
